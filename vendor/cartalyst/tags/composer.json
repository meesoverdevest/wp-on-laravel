{
    "name": "cartalyst/tags",
    "description": "Easily add tags to your Eloquent models.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "tags"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.4.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.4",
        "orchestra/database": "^3.3@dev",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": { "Cartalyst\\Tags\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Cartalyst\\Tags\\Tests\\": "tests/" }
    },
    "extra": {
        "component": "package",
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
