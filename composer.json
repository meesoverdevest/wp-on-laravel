{
    "name": "meesoverdevest/wp_on_laravel",
    "description": "A package that syncs WordPress posts, categories and tags with your laravel project.",
    "keywords": ["WordPress", "Laravel", "Package"],
    "license": "MIT",
    "require": {
        "cartalyst/tags": "5.0.*",
        "php": ">=7.0.0",
        "illuminate/database": "*"
    },
    "authors": [
        {
            "name": "biertjesman",
            "email": "mees@mefolio.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "meesoverdevest\\wp_on_laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "meesoverdevest\\wp_on_laravel\\WPServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}

